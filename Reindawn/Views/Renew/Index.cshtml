@model IEnumerable<Reindawn.Models.RenewViewModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Renewals</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Renew Records
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <table width="100%" class="table table-striped table-bordered table-hover" id="ItemTable">
                    <thead>
                        <tr>
                            <th class="col-lg-2">@Html.DisplayNameFor(model => model.PawnTicketNo)</th>
                            <th class="col-lg-4">@Html.DisplayNameFor(model => model.Date)</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@Html.DisplayFor(modelItem => item.PawnTicketNo)</td>
                                <td>@Html.DisplayFor(modelItem => item.Date)</td>
                            </tr>
                        }
                    </tbody>
                </table>
                <!-- /.table-responsive -->
                <div class="well">
                    <a class="btn btn-primary" href="@Url.Action("Create", "Renew")">Create</a>
                    @*<a class="btn btn-primary" href="@Url.Action("Edit", "Renew")">Edit</a>
                    <a class="btn btn-primary" href="@Url.Action("Delete", "Renew")">Delete</a>
                    <a class="btn btn-primary" href="@Url.Action("View", "Renew")">View</a>*@
                    @*<a class="btn btn-primary" href="#" id="btnCreateAppraiserSlip">Create Appraiser Slip</a>*@
                </div>
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
@section scripts {
    <script>
        $(document).ready(function () {
        });


    </script>
}