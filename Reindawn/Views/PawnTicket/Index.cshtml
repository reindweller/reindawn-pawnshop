
@model Reindawn.Models.PawnTicketViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Pawn Ticket</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Basic Form Elements
            </div>
            <div class="panel-body">

                <!-- Nav tabs -->
                <ul class="nav nav-tabs">
                    <li class="active">
                        <a href="#home" data-toggle="tab">Jewellry</a>
                    </li>
                    <li>
                        <a href="#home" data-toggle="tab">Gadget</a>
                    </li>
                </ul>
                <!-- Tab panes -->
                <div class="tab-content">
                    <div class="tab-pane fade in active" id="home">
                        &nbsp;
                        <form role="form">
                            <div class="row">
                                <div class="col-lg-6">

                                    <div class="form-group">
                                        @Html.LabelFor(model => model.DateLoanGranted)
                                        @Html.EditorFor(model => model.DateLoanGranted, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.DateLoanGranted, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        @Html.LabelFor(model => model.ExpirationDate)
                                        @Html.EditorFor(model => model.DateLoanGranted, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.DateLoanGranted, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->


                            <div class="row">
                                <div class="col-lg-6">

                                    <div class="form-group">
                                        @Html.LabelFor(model => model.MaturityDate)
                                        @Html.EditorFor(model => model.MaturityDate, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.MaturityDate, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        @Html.LabelFor(model => model.AuctionSale)
                                        @Html.EditorFor(model => model.AuctionSale, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.AuctionSale, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->

                            <div class="row">
                                <div class="col-lg-3">

                                    <div class="form-group">
                                        @Html.LabelFor(model => model.Prefix)
                                        @Html.EditorFor(model => model.Prefix, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.Prefix, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-3 (nested) -->
                                <div class="col-lg-3">

                                    <div class="form-group">
                                        @Html.LabelFor(model => model.FirstName)
                                        @Html.EditorFor(model => model.FirstName, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.FirstName, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-3 (nested) -->
                                <div class="col-lg-3">

                                    <div class="form-group">
                                        @Html.LabelFor(model => model.MiddleName)
                                        @Html.EditorFor(model => model.MiddleName, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.MiddleName, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-3 (nested) -->
                                <div class="col-lg-3">

                                    <div class="form-group">
                                        @Html.LabelFor(model => model.LastName)
                                        @Html.EditorFor(model => model.LastName, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.LastName, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-3 (nested) -->
                            </div>
                            <!-- /.row (nested) -->

                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="form-group">
                                        @Html.LabelFor(model => model.Address)
                                        @Html.EditorFor(model => model.Address, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.Address, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-12 (nested) -->
                            </div>
                            <!-- /.row (nested) -->

                            <div class="row">
                                <div class="col-lg-6">

                                    <div class="form-group">
                                        @Html.LabelFor(model => model.Principal)
                                        @Html.EditorFor(model => model.Principal, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.Principal, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        @Html.LabelFor(model => model.Interest)
                                        @Html.EditorFor(model => model.Interest, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.Interest, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->

                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="form-group">
                                        @Html.LabelFor(model => model.Description)
                                        @Html.EditorFor(model => model.Description, new {htmlAttributes = new {@class = "form-control datepicker"}})
                                        @Html.ValidationMessageFor(model => model.Description, "", new {@class = "text-danger"})
                                    </div>
                                </div>
                                <!-- /.col-lg-12 (nested) -->
                            </div>
                            <!-- /.row (nested) -->

                            <button type="submit" class="btn btn-default">Submit Button</button>
                            <button type="reset" class="btn btn-default">Reset Button</button>
                        </form>
                        <!-- /.form -->
                    </div>
                </div>
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->